{
    "name": "auth-strategies-benchmark",
    "version": "1.0.0",
    "description": "Benchmark test suite for JWT-only, Session-only, and Hybrid authentication strategies",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node server.js",
        "test:jwt": "node benchmarks/test-jwt.js",
        "test:session": "node benchmarks/test-session.js",
        "test:hybrid": "node benchmarks/test-hybrid.js",
        "test:all": "node benchmarks/run-all-tests.js",
        "load:jwt": "node benchmarks/load-test-jwt.js",
        "load:session": "node benchmarks/load-test-session.js",
        "load:hybrid": "node benchmarks/load-test-hybrid.js",
        "setup:db": "node scripts/setup-database.js",
        "setup:postgres": "bash scripts/setup-postgres.sh",
        "setup:docker": "bash scripts/setup-docker-postgres.sh",
        "start:postgres": "bash scripts/start-postgres.sh",
        "cleanup": "node scripts/cleanup-database.js",
        "cleanup:test": "node scripts/cleanup-database.js --test-only",
        "cleanup:all": "node scripts/cleanup-database.js --all",
        "verify": "node scripts/verify-setup.js"
    },
    "keywords": [
        "authentication",
        "jwt",
        "sessions",
        "benchmark",
        "performance"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.11.3",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.3.1"
    },
    "devDependencies": {
        "autocannon": "^7.12.0"
    }
}